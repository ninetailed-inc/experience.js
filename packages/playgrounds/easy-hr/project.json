{
  "name": "playgrounds-easy-hr",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/playgrounds/easy-hr",
  "projectType": "application",
  "targets": {
    "clean-cache": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "packages/playgrounds/easy-hr",
        "command": "rm -rf .next/cache",
        "description": "Clean Next.js build cache"
      }
    },
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/packages/playgrounds/easy-hr"
      },
      "configurations": {
        "development": {
          "outputPath": "packages/playgrounds/easy-hr"
        },
        "production": {}
      }
    },
    "serve": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "playgrounds-easy-hr:build",
        "dev": true,
        "port": 4220
      },
      "configurations": {
        "development": {
          "buildTarget": "playgrounds-easy-hr:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "playgrounds-easy-hr:build:production",
          "dev": false
        }
      }
    },
    "export": {
      "executor": "@nx/next:export",
      "options": {
        "buildTarget": "playgrounds-easy-hr:build:production"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/playgrounds/easy-hr/jest.config.js"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    }
  },
  "tags": []
}
